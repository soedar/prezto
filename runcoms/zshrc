# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.soedy/zsh/prezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.soedy/zsh/prezto/init.zsh"
fi

# Customize to your needs...
if [[ -s "${ZDOTDIR:-$HOME}/.zshrc.local" ]]; then
  source "${ZDOTDIR:-$HOME}/.zshrc.local"
fi
